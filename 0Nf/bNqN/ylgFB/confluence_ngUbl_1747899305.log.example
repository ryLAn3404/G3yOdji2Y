# your domain, e.g https://example.com
APP_URL=http://localhost:3000

APP_SECRET=REPLACE_WITH_LONG_SECRET

JWT_TOKEN_EXPIRES_IN=30d

REDIS_URL=redis://127.0.0.1:6379
# options: local | s3
STORAGE_DRIVER=local

# S3 driver config
AWS_S3_SECRET_ACCESS_KEY=
